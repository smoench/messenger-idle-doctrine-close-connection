{
  "name": "smoench/messenger-idle-doctrine-close-connection",
  "description": "Provides an idle based doctrine close connection",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "symfony/doctrine-bridge": "^4.4|^5.0",
    "symfony/event-dispatcher": "^4.4|^5.0",
    "symfony/messenger": "^4.4|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Smoench\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Smoench\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5.0"
  }
}
